<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">
    <f:entry title="${%Max messages target}"
             field="value">
        <f:textbox/>
    </f:entry>
    <f:entry title="${%Message threshold level}"
             field="threshholdlevel">
        <f:select/>
    </f:entry>
    <f:entry title="${%Continuous improvement}"
             field="improvement">
        <f:checkbox/>
    </f:entry>

</j:jelly>